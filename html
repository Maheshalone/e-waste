  <script>
    function showMorePosts() {
      // Logic to display 1-10 posts
      // Add your implementation here
    }

    function searchPosts() {
      var searchInput = document.getElementById('search-input').value.toLowerCase();
      var postContent = document.querySelector('.post-content');
      var posts = postContent.querySelectorAll('.post-image-wrap');

      posts.forEach(function(post) {
        var postTitle = post.querySelector('.post-title').textContent.toLowerCase();
        var displayStyle = 'none';

        if (postTitle.includes(searchInput)) {
          displayStyle = 'block';
        }

        post.style.display = displayStyle;
      });
    }
  </script>
